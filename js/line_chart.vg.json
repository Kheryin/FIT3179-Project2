{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1000,
  "height": 500,
  "description": "Stock prices of 5 Tech Companies over Time.",
  "data": {"url": "https://raw.githubusercontent.com/Kheryin/FIT3179-Project2/main/data/AustraliaLivestockExports3.csv"},

  "transform": [
    {
      "joinaggregate": [
        {
       "op": "sum",
       "field": "Quantity",
       "as": "Quantity"
      }
      ],
      "groupby": ["Date", "Species"]
    },
 {
 "calculate": "timeParse(datum['Date'], '%d/%m/%Y')",
 "as": "Date"
 }
 ],

  "mark": "line",
  "encoding": {
    "x": {"field": "Date","timeUnit": "yearmonth",
    "axis": {"tickCount": 10, "grid": false}},
    "y": {"field": "Quantity", "type": "quantitative"},
    "color": {"field": "Species", "type": "nominal"}
  }
}
